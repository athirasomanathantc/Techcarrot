{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "agi-intranet-custom-branding-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/agiIntranetCustomBranding/AgiIntranetCustomBrandingApplicationCustomizer.js",
          "manifest": "./src/extensions/agiIntranetCustomBranding/AgiIntranetCustomBrandingApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "AGIIntranet": {
      "path": "./src/HelperJS/AGIIntranet.Injector.js",
      "globalName": "AGIIntranet"
    }
  },
  "localizedResources": {
    "AgiIntranetCustomBrandingApplicationCustomizerStrings": "lib/extensions/agiIntranetCustomBranding/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js"
  }
}